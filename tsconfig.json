{
    "compilerOptions": {
        "noEmitOnError": true,
        "noEmitHelpers": true,
        "noUnusedLocals": true,
        "experimentalDecorators": true,
        "noLib": false,
        "sourceMap": false,
        "target": "es5",
        "module": "commonjs",
        "outDir": "bin/dist",
        "rootDir": "./"
    }, 
    "exclude": [
        "node_modules/**/*.*", 
        "demo/**/*.*", 
        "bin/**/*.*"
    ]
}